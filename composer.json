{
    "name": "felixfrey/laravel-theme",
    "description": "Theme support for Laravel with assets, theme extends etc.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "theme",
        "layout",
        "asset"
    ],
    "homepage": "https://github.com/felixfrey/laravel-theme",
    "authors": [
        {
            "name": "Felix Frey",
            "email": "mail@felixfrey.at"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/config": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/container": "^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.38"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "FelixFrey\\LaravelTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "ThemeLoader": "FelixFrey\\LaravelTheme\\Facades\\ThemeLoader"
            },
            "providers": [
                "FelixFrey\\LaravelTheme\\ThemeServiceProvider"
            ]
        }
    },
    "scripts": {
        "style:fix": "vendor/bin/php-cs-fixer fix --config php-cs-fixer.dist.php"
    }
}
